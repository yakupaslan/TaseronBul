@model List<TaseronBul.Models.AlinanIs>
@{
    ViewBag.Title = "AlinanIs";
}
<br />
<div class="col-md-4">
    <ul class="nav nav-pills nav-stacked">
        <li ><a href='@Url.Action("Kontrol", "Profil")'>Bilgilerim</a></li>
        <li><a href='@Url.Action("GuncelTeklif", "Profil")'>Güncel Teklifler</a></li>
         <li class="active"><a href='@Url.Action("AlinanIs", "Profil")'>Alınan İşler</a></li>
        <li><a href='@Url.Action("listeilan", "listeilan")'>İlan Ara</a></li>
    </ul>
</div>
<div class="col-md-8">
     <br />
     <div class="panel panel-primary">
            <!-- Default panel contents -->
            <div class="panel-heading">Kazanan Tekliflerim</div>
            <table class="table">
                <tr>
                    <td><b>Açıklama</b></td>
                    <td><b>Uye</b></td>
                    <td><b>Son Tarih</b></td>
                    <td><b>Telefon</b></td>
                    <td><b>Fiyat</b> </td>
                </tr>
                @for (int i = 0; i < Model.Count; i++)
                {
                    <tr>
                        <td>             
                            @Html.DisplayFor(modelItem => Model[i].Aciklama)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => Model[i].Uye)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => Model[i].SonTarih)
                        </td>
                          <td>
                            @Html.DisplayFor(modelItem => Model[i].Telefon)
                        </td>
                         <td>
                            @Html.DisplayFor(modelItem => Model[i].Fiyat)
                        </td>
                        <td>
                            <p>
                                <a class="btn btn-danger btn xs" ; href='@Url.Action("IlanKontT", "Ilan", new { id = Model[i].IlanNo })'>İlan Ayrıntı </a>
                            </p>
                    </tr>
                }

            </table>
        </div>
</div>
